data <- c(5, 10, 11, 13, 15, 35, 50, 55, 72, 92, 204, 215)
bin_size <- 4
n_bins <- (length(data) %/% bin_size)
binned_data_equi_depth <- cut(data, breaks = quantile(data, probs = seq(0, 1, length.out = n_bins + 1)), include.lowest = TRUE)
data_range <- range(data)
bin_width <- (data_range[2] - data_range[1]) %/% n_bins
binned_data_equi_width <- cut(data, breaks = seq(data_range[1], data_range[2] + bin_width, by = bin_width), include.lowest = TRUE)
cat("Equal-frequency (equi-depth) partitioning:\n")
print(binned_data_equi_depth)
cat("\nEqual-width partitioning:\n")
print(binned_data_equi_width)
